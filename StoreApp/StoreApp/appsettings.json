{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Messaging": {
    "StoreCode": "StoreOne",
    "URL": "amqp://guest:guest@localhost:5672",
    "StoreDirectExchange": "ws.comm",
    "ToWarehouse": {
      "RoutingKey": "s2w",
      "OrderCreationQuery": "order.creation"
    },
    "FromWarehouse": {
      "RoutingKey": "w2s",
      "OrderCancelArgument": "cancel",
      "OrderStatusUpdateArgument": "statusupdate"
    }
  }
}
